&star_job
      load_saved_model = .true.
      load_model_filename = 'start.mod'

      save_model_when_terminate = .true.
      save_model_filename = 'final.mod'

      new_core_mass = 1d2
      relax_core = .true.
      relax_initial_core = .true.
      dlg_core_mass_per_step = 1d-2 !If you get convergence issues when changing new_core_mass or initial_mass, try decreasing dlg_core_mass_per_step

      set_initial_model_number = .true.
      initial_model_number = 0

      ! display on-screen plots
      pgstar_flag = .true.

      required_termination_code_string = 'extras_check_model'

/ ! end of star_job namelist

&eos

/ ! end of eos namelist

