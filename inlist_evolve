    ! limit max_model_number as part of test_suite
	max_number_retries = 1500
    ! Enable or disable wind
    ! use_other_wind = .true.
    
    x_logical_ctrl(1) = .true.  ! accrete onto black hole 
    x_logical_ctrl(3) = .true.  ! enable timestep slowing
